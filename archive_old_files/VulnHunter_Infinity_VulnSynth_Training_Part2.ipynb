    "                '(', ')', '{', '}', '[', ']', ';', '=', '+', '-'\n",
    "            ]\n",
    "            return c_vocab[token_id % len(c_vocab)]\n",
    "        \n",
    "        elif language == 'python':\n",
    "            python_vocab = [\n",
    "                'def', 'class', 'if', 'else', 'for', 'while', 'return',\n",
